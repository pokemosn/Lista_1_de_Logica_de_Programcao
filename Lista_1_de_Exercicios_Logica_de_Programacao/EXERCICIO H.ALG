Algoritmo "EXERCICIO H"

Var
salario, salario_final, reajuste2: real

Inicio
escreva ("Salario do funcionaro:")
leia (salario)
escreva ("Valor do reajuste no salario:")
leia (reajuste2)
reajuste2 <- reajuste2/100
salario_final <- salario + reajuste2
escreva ("O salario final do funcionario é:", salario_final)


Fimalgoritmo